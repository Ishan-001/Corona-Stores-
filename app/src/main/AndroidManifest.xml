<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.measures.corona_hack">

    <application
        android:allowBackup="true"
        android:icon="@drawable/store_icon"
        android:label="@string/app_name"
        android:roundIcon="@drawable/store_icon"
        android:supportsRtl="true"
        android:theme="@style/AppTheme2">
        <activity android:name=".Login"></activity>
        <activity android:name=".DetailsActivity" />
        <activity android:name=".CustomerActivity" />
        <activity
            android:name=".Register"
            android:label="Register" />
        <activity android:name=".StartActivity" />
        <activity
            android:name=".OwnerActivity"
            android:label="@string/title_activity_owner" />
        <activity
            android:name=".MainActivity"
            android:theme="@style/AppTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>